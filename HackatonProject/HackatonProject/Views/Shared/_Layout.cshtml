<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/materialize.css" rel="stylesheet" />
    <link href="~/Content/font-awesome.css" rel="stylesheet" />
    <link href="~/Content/estilos.css" rel="stylesheet" />
</head>
<body>


    <nav class="green lighten-3">
        <div class="nav-wrapper">
            <a href="#!" class="brand-logo">Logo</a>
            <a href="#" data-target="mobile-demo" class="sidenav-trigger"><i class="fa fa-list"></i></a>
            <ul class="right hide-on-med-and-down">
                <li><a href="@Url.Action("Login","Home")">Log In</a></li>
                <li><a href="@Url.Action("SignUp","Home")">Sign Up</a></li>
                <li><a href="@Url.Action("MapaDeBusqueda","Home")">Productos</a></li>
            </ul>
        </div>
    </nav>

    <ul class="sidenav" id="mobile-demo">
        <li><a href="@Url.Action("Login","Home")">Log In</a></li>
        <li><a href="@Url.Action("SignUp","Home")">Sign Up</a></li>
        <li><a href="@Url.Action("MapaDeBusqueda","Home")">Productos</a></li>
    </ul>

    <div class="container body-content" id="app">
        @RenderBody()

        <footer class="page-footer teal green lighten-3">

            <div class="footer-copyright green lighten-3">
                <div class="container">
                    Made by Tec TEAM
                </div>
            </div>
        </footer>

    </div>

    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)
    <script src="~/Scripts/materialize.js"></script>
    <script src="~/Scripts/axios.js"></script>
    <script src="~/Scripts/vue.js"></script>
    <script src="~/Scripts/vue-sessionstorage.min.js"></script>
    <script src="~/Scripts/vue-cookie.js"></script>
    <script src="~/Scripts/vue-google-maps.js"></script>
    <script src="~/app/app.js"></script>
</body>
</html>
